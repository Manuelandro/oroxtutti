{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "Nome": {
      "type": "string",
      "required": true
    },
    "Descrizione": {
      "type": "string",
      "required": true
    },
    "Prezzo": {
      "type": "float"
    },
    "Foto": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "Slug": {
      "type": "uid",
      "targetField": "Nome"
    },
    "Categoria": {
      "collection": "category",
      "via": "products"
    },
    "Stock": {
      "type": "integer"
    },
    "Codice": {
      "type": "string"
    }
  }
}
